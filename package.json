{
  "name": "samnivesh-react",
  "version": "1.0.0",
  "main": "build/server/index.js",
  "license": "MIT",
  "scripts": {
    "start": "node build/server/index",
    "test": "jest",
    "dev": "NODE_ENV=development NODE_PATH=./src nodemon  --exec babel-node  src/server/index.js --ignore '*.css'",
    "webpack": "webpack -wd",
    "build-webpack": "webpack -p",
    "build-node": "babel src -d build --copy-files --ignore",
    "start-prod": "NODE_ENV=production NODE_PATH=./build pm2-runtime start ecosystem.config.js --env production",
    "build": "yarn run build-webpack && yarn run build-node"
  },
  "dependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "babel-node": "^0.0.1-security",
    "babel-polyfill": "^6.26.0",
    "bulma": "0.7.4",
    "core-js": "^3.3.5",
    "css-loader": "^3.2.0",
    "ejs": "^2.7.1",
    "express": "^4.17.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.1.2",
    "style-loader": "^1.0.0",
    "webpack": "^4.39.2",
    "webpack-cli": "^3.3.6",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "^6.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.9.0"
  },
  "engines": {
    "node": ">=10.15.2",
    "npm": ">=5.8.0"
  }
}
